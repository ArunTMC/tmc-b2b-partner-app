<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginTop="11dp"
        android:layout_marginStart="15dp"
        android:id="@+id/parent_card_view"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="10dp"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:padding="10dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_marginBottom="10dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/orderid"
                        android:textSize="14sp"
                        android:textColor="@color/TMCPrimaryPurpleColor"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:layout_marginBottom="7dp"
                        android:fontFamily="@font/open_sans_bold"
                        android:text=" "
                        android:id="@+id/orderid_textview"
                        android:textColor="@color/black"/>
                </LinearLayout>



            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_marginBottom="10dp"

                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/retailername"
                        android:textColor="@color/TMCPrimaryPurpleColor"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/retailername_textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginBottom="7dp"
                        android:fontFamily="@font/open_sans_bold"

                        android:text=" "
                        android:textColor="@color/black"
                        android:textSize="12sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/retailermobileno"
                        android:textSize="14sp"
                        android:textColor="@color/TMCPrimaryPurpleColor"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:text=" "
                        android:layout_marginBottom="7dp"
                        android:fontFamily="@font/open_sans_bold"

                        android:id="@+id/retailer_mobileno_textview"
                        android:textColor="@color/black"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginBottom="10dp"

                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/placedtime"
                    android:textSize="14sp"
                    android:textColor="@color/TMCPrimaryPurpleColor"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="12sp"
                    android:text=" "
                    android:layout_marginBottom="7dp"
                    android:fontFamily="@font/open_sans_bold"

                    android:id="@+id/orderplacedtime_textview"
                    android:textColor="@color/black"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/paymentmode"
                        android:textSize="14sp"
                        android:textColor="@color/TMCPrimaryPurpleColor"/>

                    <TextView
                        android:id="@+id/paymentmode_textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text=" "
                        android:layout_marginBottom="7dp"
                        android:gravity="start"
                        android:fontFamily="@font/open_sans_bold"

                        android:textColor="@color/black"
                        android:textSize="12sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/payableamount"
                        android:textSize="14sp"
                        android:textColor="@color/TMCPrimaryPurpleColor"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:text=" "
                        android:layout_marginBottom="7dp"
                        android:gravity="start"
                        android:fontFamily="@font/open_sans_bold"

                        android:id="@+id/payableAmount_textview"
                        android:textColor="@color/black"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/MALEQUANTITY"
                        android:textSize="14sp"
                        android:gravity="left"
                        android:textColor="@color/TMCPrimaryPurpleColor"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginBottom="7dp"
                        android:gravity="left"
                        android:layout_marginEnd="20dp"
                        android:fontFamily="@font/open_sans_bold"
                        android:textSize="12sp"
                        android:layout_marginStart="5dp"
                        android:text="  "
                        android:id="@+id/male_quantity_textview"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/FEMALEQUANTITY"
                        android:textSize="14sp"
                        android:gravity="center"

                        android:textColor="@color/TMCPrimaryPurpleColor"/>

                    <TextView
                        android:id="@+id/female_quantity_textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginBottom="7dp"
                        android:gravity="center"
                        android:fontFamily="@font/open_sans_bold"

                        android:text=" "
                        android:textColor="@color/black"
                        android:textSize="12sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/TOTALQUANTITY"
                        android:textSize="14sp"
                        android:gravity="center"

                        android:textColor="@color/TMCPrimaryPurpleColor"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:text=" "
                        android:layout_marginBottom="7dp"
                        android:gravity="center"
                        android:fontFamily="@font/open_sans_bold"

                        android:id="@+id/total_qty_textview"
                        android:textColor="@color/black"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/MALEWEIGHT"
                        android:textSize="14sp"
                        android:gravity="left"

                        android:textColor="@color/TMCPrimaryPurpleColor"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:text=" "
                        android:layout_marginBottom="7dp"
                        android:gravity="left"
                        android:layout_marginStart="5dp"

                        android:fontFamily="@font/open_sans_bold"

                        android:id="@+id/male_weight_textview"
                        android:textColor="@color/black"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/FEMALEWEIGHT"
                        android:textSize="14sp"
                        android:gravity="center"

                        android:textColor="@color/TMCPrimaryPurpleColor"/>

                    <TextView
                        android:id="@+id/female_weight_textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginBottom="7dp"
                        android:gravity="center"
                        android:fontFamily="@font/open_sans_bold"

                        android:text=" "
                        android:textColor="@color/black"
                        android:textSize="12sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/TOTALWEIGHT"
                        android:textSize="14sp"
                        android:gravity="center"

                        android:textColor="@color/TMCPrimaryPurpleColor"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:layout_marginBottom="7dp"
                        android:gravity="center"
                        android:fontFamily="@font/open_sans_bold"

                        android:text=" "
                        android:id="@+id/total_weight_textview"
                        android:textColor="@color/black"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </androidx.cardview.widget.CardView>


</LinearLayout>